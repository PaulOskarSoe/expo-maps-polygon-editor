{
  "name": "@siposdani87/expo-maps-polygon-editor",
  "version": "0.2.0",
  "description": "This multi polygon editor written in TypeScript for the React Native Maps component. This component allows you to select, create, or modify areas on Apple Maps and Google Maps.",
  "author": "Dániel Sipos",
  "license": "MIT",
  "homepage": "https://siposdani87.hu",
  "private": false,
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "keywords": [
    "expo-maps-polygon-editor",
    "react-native-maps",
    "polygon",
    "editor"
  ],
  "files": [
    "dist",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/siposdani87/expo-maps-polygon-editor.git"
  },
  "bugs": {
    "url": "https://github.com/siposdani87/expo-maps-polygon-editor/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prebuild": "npm run clean",
    "build": "tsc -p .",
    "clean": "rm -rf dist",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --write 'src/**/*.ts*'"
  },
  "peerDependencies": {
    "react": ">=17.0",
    "react-native": "*",
    "react-native-maps": "*",
    "@turf/boolean-point-in-polygon": "*",
    "@turf/helpers": "*",
    "@turf/midpoint": "*"
  },
  "devDependencies": {
    "@react-native-community/eslint-config": "^3.0.1",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.6.2",
    "@types/react": "~17.0.21",
    "@types/react-dom": "~17.0.9",
    "@types/react-native": "~0.64.12",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "eslint": "^7.32.0",
    "expo": "^43.0.0",
    "prettier": "^2.4.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-native": "0.64.2",
    "@turf/boolean-point-in-polygon": "6.5.0",
    "@turf/helpers": "6.5.0",
    "@turf/midpoint": "6.5.0",
    "react-native-maps": "0.28.0",
    "typescript": "~4.3.5"
  }
}
